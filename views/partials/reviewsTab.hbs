<div class="container">
  <div class="row">
            {{#each reviews}}
              <div class="container">
                <h3>{{course.courseNumber}}: {{courseTitle}}</h3>
                <h4>{{faculty.name}}, {{semester}}</h4>
                <h5>{{rating}}</h5>
                <p>{{description}}</p>
                <p>{{review}}</p>
              </div>
            {{/each}}
  </div>
</div>
